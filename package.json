{
  "name": "node-redux",
  "version": "1.0.0",
  "description": "template for Node, Mongo, React, Redux apps",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "rm -rf dist && babel src --out-dir dist",
    "start": "NODE_ENV=production PORT=3333cs pm2 start dist/index.js",
    "test": "mocha  src/tests/**/*.js  --timeout 10000 -c --compilers js:babel-core/register || true",
    "test:nyan": "mocha src/tests/**/*.js  --timeout 10000 -c -R nyan --compilers js:babel-core/register || true",
    "lint": "eslint src"
  },
  "keywords": [
    "Express"
  ],
  "author": "Vincenzo Bianco",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^3.3.0",
    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "crypto": "0.0.3",
    "express": "^4.13.4",
    "express-validation": "^1.0.1",
    "http-status": "^0.2.3",
    "joi": "^9.2.0",
    "joi-objectid": "^2.0.0",
    "jsonwebtoken": "^7.3.0",
    "jwt-simple": "^0.5.0",
    "method-override": "^2.3.6",
