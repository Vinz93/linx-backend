{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-console": 0,
    "func-names": 0,
    "max-len": 0,
    "quotes": 0,
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "_id",
          "__v",
          "__t",
          "_require",
          "_model"
        ]
      }
    ],
    "no-param-reassign": [2, { "props": false }],
    "import/no-unresolved": 0
  },
   "env": {
		"node": true
	}
}
